<h1>Listing events</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Start Date</th>
    <th>End Date</th>
  </tr>
<% @events.each do |event| %>
  <tr>
    <td><%= event.description %></td>
    <td><%= event.startdate %></td>
    <td><%= event.enddate %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>